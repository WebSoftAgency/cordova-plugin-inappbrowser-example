<?xml version='1.0' encoding='utf-8'?>
  <widget id="com.example.sorfal" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>cordova-plugin-inappbrowser-example</name>
    <description>
      A sample application uses cordova-plugin-inappbrowser.
    </description>
    <author email="sorfal.web@gmail.comg" href="http://cordova.io">
      Nikita Isaev
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
      <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
      <allow-intent href="itms:*" />
      <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-inappbrowser" spec="^1.0.1" />
    <plugin name="cordova-plugin-crosswalk-webview" spec="^1.3.1" />
    <plugin name="cordova-plugin-whitelist" spec="^1.1.0" />
    <preference name="xwalkVersion" value="14+" />
    <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect" />
    <preference name="xwalkMode" value="embedded" />
    <plugin name="cordova-plugin-network-information" spec="^1.0.1" />
  </widget>
